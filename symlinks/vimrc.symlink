" General {
    set nocompatible
    filetype plugin indent on
" }

" Vim UI {
    set number
    syntax enable
    colorscheme desert
    set autoindent
    set smartindent
    set shiftwidth=4 "autoindent width
    set tabstop=4
    set scrolloff=10
    set showmatch "show brace/paranthese/bracket match
" }